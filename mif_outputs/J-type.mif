0040 : 00001000000000000000000000101010;
    -- bne $20, $21, -2 		# pc = 41+1-2 = 40
0041 : 00010101001010111111111111111110;
    -- addi $1, $0, 10086      # r1 = 65535 = 0x0000FFFF
0042 : 00101000010000000010011101100110;
[0043 .. 4095] : 00000000000000000000000000000000;
